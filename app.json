{
  "expo": {
    "name": "demo_salidas",
    "slug": "demo_salidas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["../assets/fonts/Termina-Regular.otf", "../assets/fonts/NeueHaasUnicaW1G-Regular.otf", "../assets/fonts/Termina-Heavy.otf", "../assets/fonts/Termina-Bold.otf"]
        }
      ]
    ]
  }
}
